      "prettier --write"
    ],
    "{config,src}/**/*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "{config,src}/**/*.{css,scss}": [
      "prettier --write",
      "stylelint --fix"
    ]
  }
